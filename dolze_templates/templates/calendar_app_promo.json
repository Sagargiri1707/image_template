{
  "name": "calendar_app_promo",
  "description": "Clean promotional template featuring a calendar app interface",
  "size": { "width": 2048, "height": 2048 },
  "use_base_image": false,
  "components": [
    {
      "type": "rectangle",
      "position": { "x": 0, "y": 0 },
      "size": { "width": 2048, "height": 2048 },
      "fill_color": "${theme_color}",
      "outline_color": null
    },
    {
      "type": "text",
      "text": "${heading}",
      "position": { "x": 114, "y": 152 },
      "font_size": 182,
      "color": [52, 64, 84, 1],
      "font_path": "Outfit-Regular",
      "alignment": "left",
      "max_width": 1138
    },
    {
      "type": "image",
      "image_url": "${logo_url}",
      "position": { "x": 114, "y": 1422 },
      "size": { "width": 228, "height": 228 },
      "circle_crop": true
    },
    {
      "type": "image",
      "image_url": "${image_url}",
      "position": { "x": 758, "y": 683 },
      "size": { "width": 1157, "height": 2405 },
      "border_radius": 38
    },
    {
      "type": "text",
      "text": "${website_url}",
      "position": { "x": 114, "y": 1726 },
      "font_size": 61,
      "color": [52, 64, 84, 1],
      "font_path": "Outfit-Regular",
      "alignment": "left",
      "max_width": 95
    }
  ]
}
